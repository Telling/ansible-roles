[global]
pid = run/php-fpm.pid
error_log = syslog
syslog.facility = local3

[www]
user = www
group = www
listen = {{ php_fpm_socket_path }}
listen.owner = www
listen.group = www

pm = dynamic
pm.max_children = 32
pm.start_servers = 16
pm.min_spare_servers = 4
pm.max_spare_servers = 32

catch_workers_output = yes

